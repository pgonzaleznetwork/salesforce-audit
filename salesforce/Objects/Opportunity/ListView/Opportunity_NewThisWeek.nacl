salesforce.ListView Opportunity_NewThisWeek {
  fullName = "Opportunity.NewThisWeek"
  filterScope = "Everything"
  filters = [
    {
      field = "OPPORTUNITY.CREATED_DATE"
      operation = "equals"
      value = "THIS_WEEK"
    },
  ]
  label = "New This Week"
  _parent = [
    salesforce.Opportunity,
  ]
}
