salesforce.ListView Event_MyRecentEvents {
  fullName = "Event.MyRecentEvents"
  columns = [
    "START_DATE_TIME",
    "END_DATE_TIME",
    "SUBJECT",
    "WHO_NAME",
    "WHAT_NAME",
    "CORE.USERS.ALIAS",
    "ALL_DAY",
  ]
  filterScope = "Mine"
  filters = [
    {
      field = "DUE_DATE"
      operation = "equals"
      value = "LAST_N_DAYS:60"
    },
  ]
  label = "My Recent Events"
  _parent = [
    salesforce.Event,
  ]
}
